{
    "name": "Auto Referer",
    "description": "Control referer to protect privacy and not break web",
    "manifest_version": 2,
    "version": "0.6.2",
    "homepage_url": "https://github.com/garywill/autoreferer",
    "developer": {
        "name": "Garywill",
        "url": "https://github.com/garywill/autoreferer"
    },
    "permissions": [
        "webRequest",
        "webRequestBlocking",
        "*://*/*"
    ],
    "applications": {
        "gecko": {
            "strict_min_version": "63.0"
        }
    },
    "icons": {
        "96": "icon.svg"
    },
    "background": {
        "scripts": ["background.js"]
    },
    "browser_action": {
        "default_icon": "icon.svg",
        "default_title": "Auto Referer"
    }

} 
